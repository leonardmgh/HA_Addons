# Automatically generated do not edit

no-resolv
no-hosts
keep-in-foreground
log-queries
log-facility=-
no-poll
dhcp-no-override
user=root

# disable dns server
port=0

# enable tftp server
enable-tftp
tftp-root=/config{{ .TFTPpath }}

# dhcp proxy mode. do not act as the authoratitive dhcp server
{{ if .DHCPPROXY }}
dhcp-range={{ .DHCPserver }},proxy
{{ else }}
{{ range .dhcprange}}
dhcp-range={{ .dhcprange }}
{{ end }}
{{ end }}

{{ if .MACROOT }}
tftp-unique-root=mac
{{ end }}

# enable logging
log-queries
log-dhcp

{{ range .dnsmasqoptions }}
{{ . }}
{{ end }}