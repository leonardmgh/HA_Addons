# Automatically generated do not edit

no-resolv
no-hosts
keep-in-foreground
log-queries
log-facility=-
no-poll
dhcp-no-override
user=root

# disable dns server
port=0

# enable tftp server
enable-tftp
tftp-root=/config{{ .TFTPpath }}

# dhcp proxy mode. do not act as the authoratitive dhcp server
dhcp-range={{ .DHCPserver }},proxy

{{ if .MACROOT }}
tftp-unique-root=mac
{{ end }}

# enable logging
log-queries
log-dhcp

{{ range .dhcpoptions }}
dhcp-option={{ . }}
{{ end }}

{{ range .dhcpvendorclass }}
dhcp-vendorclass={{ . }}
{{ end }}

{{ range .dhcpboot }}
dhcp-boot={{ . }}
{{ end }}